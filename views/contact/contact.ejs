      <div class="card-body">
        <h4 class="card-title">show CMS</h4>
       
        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <th scope="col">S.no</th>
                <th scope="col">name</th>
                <th scope="col">subject</th>           
                <th scope="col">email</th>
                
                <th scope="col">phone</th>
                <th scope="col">message</th>
              </tr>
            </thead>
            <tbody>
              <% allContact.forEach((contact,index)=>{%>
                <tr>
                  
                    <td><%= index+1 %></td>
                    <td><%= contact.name %></td>
                    <td><%= contact.subject %></td>
                    <td><%= contact.email %></td>
                    <td><%= contact.phone %></td>
                    <td><%= contact.message %></td>
                 
                    
                      </tr>
                      <%  }) %>
                  
        
            </tbody>
          </table>
        </div>
      </div>
    </div>
