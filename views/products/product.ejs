
<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <title>Product </title>
  </head>
  <body class="bg-info">




    <div class="container mt-3 grid-margin stretch-card">
      <div class="card">
        <div class="card-body">
          <h4 class="card-title">Product</h4>
          
          <form class="forms-sample" action="productSubmit" method="post" enctype="multipart/form-data">
          


            <div class="form-group">
               <div class="row">

                   <div class="col-3">
                    <label for="exampleInputEmail1">Title</label>
                    <input type="text" name="title" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter title">
                   </div>

                   <div class="col-3">
                    <label for="exampleInputEmail1">SKU</label>
                    <input type="text" name="sku" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter SKU">
                   </div>

                   <div class="col-3">
                    <label for="exampleInputPassword1">Model Name</label>
                    <input type="text" name="modelName" class="form-control" id="exampleInputPassword1" placeholder="select model name">
                   </div>
                    
                   <div class="col-3">
                    <label for="exampleInputEmail1">exchange day</label>
                     <input type="date" name="exchangeday" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter sequence">
                   </div>

               </div>
              </div>



        <div class="form-group">
            <div class="row">

                <div class="col-3">
                    <label for="exampleInputEmail1">image</label>
                    <input type="file" name="image" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter image ">
                   </div>

                <div class="col-3">
                    <label for="exampleInputPassword1">Category</label>
    
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                          <label class="input-group-text" for="inputGroupSelect01">Category</label>
                        </div>
                        <select name="category" class="custom-select form-control" id="inputGroupSelect01">
                          <option selected>select category...</option>
                          <%category.forEach((element)=>{%>
                            <option value="<%=element.id%>"><%=element.name%></option>
                            <%})%>
                          
                         
                        </select>
            
                      </div>

                </div>
                <div class="col-3">
                    <label for="exampleInputPassword1">Sub Category</label>
    
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                          <label class="input-group-text" for="inputGroupSelect01">Sub Category</label>
                        </div>
                        <select name="subcategory" class="custom-select form-control" id="inputGroupSelect01" placeholde="Select category">
                          <option selected>select sub category...</option>
                          <%subcategory.forEach((element)=>{%>
                            <option value="<%=element.id%>"><%=element.name%></option>
                            <%})%>
                          
                         
                        </select>
            
                      </div>
                </div>
                <div class="col-3">
                    <label for="exampleInputPassword1">Child Category</label>
    
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                          <label class="input-group-text" for="inputGroupSelect01">Child Category</label>
                        </div>
                        <select name="childcategory" class="custom-select form-control" id="inputGroupSelect01">
                          <option selected>select category...</option>
                          <% childCategory.forEach((element)=>{%>
                            <option value="<%=element.id%>"><%=element.name%></option>
                            <%})%>
                          
                         
                        </select>
            
                      </div>

                </div>

            </div>
            
          </div>
          
          <div class="form-group">
             <div class="row">
               
                <div class="col-3">
                    <label for="exampleInputPassword1">Brand</label>
    
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                          <label class="input-group-text" for="inputGroupSelect01">Brand</label>
                        </div>
                        <select name="brand" class="custom-select form-control" id="inputGroupSelect01">
                          <option selected>select brand...</option>

                          <%brand.forEach((element)=>{%>
                            <option value="<%= element.id %>"><%=element.name%></option>
                            <%})%>
                           
                            
                          
                         
                        </select>
            
                      </div>
                </div>
                <div class="col-3">
                    <label for="exampleInputPassword1">Status</label>
    
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                          <label class="input-group-text" for="inputGroupSelect01">Status</label>
                        </div>
                        <select name="status" class="custom-select form-control" id="inputGroupSelect01">
                          <option selected>select status...</option>
                          <option value="1">Active</option>
                          <option value="0">Inactive</option>
                         
                        </select>
            
                      </div>
                </div>

                <div class="col-3">
                    <label for="exampleInputPassword1">GST</label>
    
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                          <label class="input-group-text" for="inputGroupSelect01">GST</label>
                        </div>
                        <select name="gst" class="custom-select form-control" id="inputGroupSelect01">
                          <option selected>select GST...</option>
                         
                          
                          <option value="10">10%</option>
                          <option value="20">20%</option>
                          <option value="30">30%</option>
                          
                        
                         
                        </select>
            
                      </div>
                
                </div>

                <div class="col-3">
                    <label for="exampleInputPassword1">Payment Mode</label>
    
                    <div class="input-group mb-3">

                        <div class="input-group-prepend">
                          <label class="input-group-text" for="inputGroupSelect01">Category</label>
                        </div>

                        <select name="paymentMode" class="custom-select form-control" id="inputGroupSelect01">
                          <option selected>select payment mode...</option>
                    
                            <option value="credit card">credit card</option>
                            <option value="debit card">debit card</option>
                            <option value="Cash">Cash</option>
                            <option value="mobile payment">Mobile payment</option>
                            <option value="Electronic bank transfer">Electronic bank transfer</option>                               
                        </select>
            
                      </div>
                   </div>
                     

             </div>
          </div>
            

          
          <div class="form-group">
                <div class="row">
                    <div class="col-6">
                        <label for="exampleInputPassword1">Summary</label>                
                        <textarea name="summary" class="form-control" id="exampleTextarea1" rows="2"></textarea>
                    </div>
                    <div class="col-6">
                        <label for="exampleInputPassword1">Description</label>                
                        <textarea name="description" class="form-control" id="exampleTextarea1" rows="2"></textarea>
                    </div>
                </div>
              </div>


          
          
              <div class="form-group">
                <div class="row">

                   <div class="col-3">
                    <label for="exampleInputEmail1">Return day</label>
                    <input type="date" name="returnDay" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter sequence">
                   </div>
                     
                  
                   <div class="col-3">
                    <label for="exampleInputEmail1">Price</label>
                    <input type="number" name="price" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter price ">
                   </div>
                     
                   <div class="col-3">
                    <label for="exampleInputEmail1">Discount</label>
                    <input type="number" name="discount" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter price ">
                   </div>
                    
                   <div class="col-3">
                    <label for="exampleInputEmail1"> Selling Price</label>
                    <input type="number" name="sellingPrice" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter price ">
                   </div>
 
                </div>
               </div>
 

            
        
        
        
            
            <button type="submit" class="btn btn-primary mr-2">add product</button>
            <button class="btn btn-light">Cancel</button>
          </form>
        </div>
      </div>
    </div>


 
  </body>
</html>